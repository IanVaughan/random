  -Ic:\mssdk\include;$(BCB)\bin;$(BCB)\lib\obj;$(BCB)\include;$(BCB)\include\vcl \
  -$Y -$W -$O- -v -JPHN -M
RFLAGS = -ic:\mssdk\include;$(BCB)\bin;$(BCB)\lib\obj;$(BCB)\include;$(BCB)\include\vcl
AFLAGS = /ic:\mssdk\include /i$(BCB)\bin /i$(BCB)\lib\obj /i$(BCB)\include \
  /i$(BCB)\include\vcl /mx /w2 /zi /dUSEPACKAGES
LFLAGS = -L$(BCB)\bin;$(BCB)\lib;$(BCB)\lib\obj;$(BCB)\lib\obj\WrapGrid;$(BCB)\lib\obj\zbuilder;$(RELEASELIBPATH) \
  -aa -Tpd -x -Gn -Gi -v
IFLAGS =
# ---------------------------------------------------------------------------
ALLOBJ = c0d32.obj sysinit.obj $(OBJFILES)
ALLRES = $(RESFILES)
ALLLIB = $(LIBFILES) $(LIBRARIES) import32.lib cp32mt.lib
